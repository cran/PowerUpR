citHeader("To cite PowerUpR in publications use:")

bibentry(bibtype = "Manual",
  title        = "{PowerUpR}: Power Analysis Tools for Multilevel Randomized Experiments",
  author       = as.person("Metin Bulus, Nianbo Dong, Benjamin Kelcey, Jessaca Spybrook"),
  note         = "R package version 1.1.0",
  year         = "2021",
  url = "https://CRAN.R-project.org/package=PowerUpR",

  textVersion  =
  paste("Bulus, M., Dong, N., Kelcey, B., & Spybrook, J. (2021).",
        "PowerUpR:  Power Analysis Tools for Multilevel Randomized Experiments.",
        "R package version 1.1.0.",
        "https://CRAN.R-project.org/package=PowerUpR")
)
