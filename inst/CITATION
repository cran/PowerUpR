citEntry(entry = "Unpublished",
  title        = "{PowerUpR}: Power Analysis Tools for Multilevel Randomized Experiments",
  author       = as.person("Metin Bulus, Nianbo Dong"),
  note         = "R package version 0.2.3",
  year         = 2018
 ,

  textVersion  =
  paste("Bulus, M., & Dong, N., (2018).",
        "PowerUpR:  Power Analysis Tools for Multilevel Randomized Experiments.",
        "R package version 0.2.3.")
)
