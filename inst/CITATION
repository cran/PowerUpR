citHeader("To cite PowerUpR in publications use:")

citEntry(entry = "Unpublished",
  title        = "{PowerUpR}: R Version of PowerUp!",
  author       = as.person("Metin Bulus, Nianbo Dong, Benjamin Kelcey, Jessaca Spybrook, Bang Le,"),
  note         = "R package version 1.1.3",
  year         = 2017
 ,

  textVersion  =
  paste("Bulus, M., Dong, N., Kelcey, B., Spybrook, J., &  Le, B., (2017).",
        "PowerUpR:  R Version of PowerUp!.",
        "R package version 1.1.3.")
)
